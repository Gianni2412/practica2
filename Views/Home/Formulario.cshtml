@model Formulario


<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" type="text/css" href="estilof.css">
    <title> CONSULTAS</title>
</head>

<body>
    <form method="post">
        <div class="text-center">
            <div class="text-center">
                <h1 class="display-4">AQUI REALIZE SUS CONSULTAS</h1>

                <div class="form-group">
                    <label asp-for="NombreCompleto"></label>
                    <input asp-for="NombreCompleto" class="form-control">
                </div>


                <div class="form-group">
                    <label asp-for="Asunto"></label>
                    <input asp-for="Asunto" class="form-control">
                </div>


                <div class="form-group">
                    <label asp-for="Mensaje"></label>
                    <input asp-for="Mensaje" class="form-control">
                </div>



                <button type="button" class="btn btn-dark">enviar consulta</button>
    </form>

</body>

</html>
